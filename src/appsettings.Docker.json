{
  "consul": {
    "enabled": true,
    "url": "http://spirebyte-consul:8500",
    "service": {
      "useDnsName": "true"
    }
  },
  "rabbitMQ": {
    "enabled": true,
    "connectionString": "host=spirebyte-rabbitmq;port=5672;virtualHost=/;username=guest;password=guest"
  },
  "serilog": {
    "seq": {
      "enabled": true,
      "url": "http://spirebyte-seq:5341",
      "apiKey": "secret"
    }
  },
  "tracing": {
    "enabled": true,
    "jaeger": {
      "agentHost": "spirebyte-jaeger",
      "agentPort": 6831
    }
  }
}
