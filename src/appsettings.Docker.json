{
  "logger": {
    "console": {
      "enabled": true
    },
    "elk": {
      "enabled": false,
      "url": "http://spirebyte-elk:9200"
    },
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://spirebyte-seq:5341",
      "apiKey": "secret"
    }
  },
  "consul": {
    "enabled": true,
    "url": "http://spirebyte-consul:8500",
    "service": "api-gateway",
    "address": "spirebyte-api-gateway",
    "port": "80"
  },
  "jwt": {
    "certificate": {
      "location": "",
      "password": "",
      "rawData": ""
    },
    "issuerSigningKey": "eiquief5phee9pazo0Faegaez9gohThailiur5woy2befiech1oarai4aiLi6ahVecah3ie9Aiz6Peij"
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "api-gateway",
    "udpHost": "spirebyte-jaeger",
    "udpPort": 6831,
    "maxPacketSize": 65000,
    "sampler": "const",
    "excludePaths": [
      "/",
      "/ping",
      "/metrics"
    ]
  },
  "metrics": {
    "tags": {
      "env": "production",
      "server": "vps"
    }
  },
  "rabbitMq": {
    "hostnames": [
      "spirebyte-rabbitmq"
    ]
  },
  "vault": {
    "enabled": false,
    "url": "http://spirebyte-vault:8200",
    "kv": {
      "enabled": false
    },
    "pki": {
      "enabled": false
    }
  }
